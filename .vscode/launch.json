{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Analyze Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "args": ["../CommonLibSSE", "analyze"]
    },

    {
      "name": "Python: Generate Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "args": ["../CommonLibVR", "generate", "--min", "1", "-d", ]
    },
    {
      "name": "Python: Generate AE Map Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "args": ["../CommonLibSSE", "generate", "--min", "1", "-d", "-m"]
    },
    {
      "name": "Python: Analyze Current File (FO4)",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "args": ["../CommonLibF4", "-f", "analyze"]
    },
    {
      "name": "Python: Generate Database (FO4)",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "args": ["../CommonLibF4", "-f", "generate", "--min", "1", "-d"]
    },


  ]
}
